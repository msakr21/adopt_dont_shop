<h1> New Application </h1>
<%= form_with :url => '/applications/new', method: :post do |form| %>
<pre>
<%= form.label :name %>

<%= form.text_field :applicant %>

<%= form.label :street %>

<%= form.text_field :street %>

<%= form.label :city %>

<%= form.text_field :city %>

<%= form.label :state %>

<%= form.text_field :state %>

<%= form.label :zipcode %>

<%= form.text_field :zipcode %>

<%= form.hidden_field :status, value:"In Progress" %>

<%= form.hidden_field :reason, value:"temp" %>

<%= "Status:" %>

<%= "In Progress" %>


<%= form.submit "Submit" %>
</pre>
<% end %>
